<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>view</key>
	<dict>
		<key>constraints</key>
		<array>
			<string>V:|-0-[background]|</string>
			<string>H:|[background]|</string>
			<string>V:|-0-[scrollView]|</string>
			<string>H:|[scrollView]|</string>
		</array>
	</dict>
	<key>scrollView</key>
	<dict>
		<key>constraints</key>
		<array>
			<string>V:|[containerView]|</string>
			<string>#containerView.width=scrollView.width,1,0</string>
			<string>#containerView.centerx=scrollView.centerx,1,0</string>
		</array>
	</dict>
	<key>containerView</key>
	<dict>
		<key>constraints</key>
		<array>
			<string>V:|-16-[parkingReminderTitle][parkingChooseMethod]-&gt;=0-|</string>
			<string>H:|-12-[parkingReminderTitle]-&gt;=1-[parkingIcon(45)]-40-|</string>
			<string>H:|-11-[parkingChooseMethod]-&gt;=1-|</string>
			<string>H:|[gpsLocationButton][takePictureButton]|</string>
			<string>H:|[recordVoiceMemoButton][writeNoteButton]|</string>
			<string>H:|-&gt;=0-[gpsLocationButtonLabel]-&gt;=0-[takePictureButtonLabel]-&gt;=0-|</string>
			<string>H:|-&gt;=0-[recordVoiceMemoButtonLabel]-&gt;=0-[writeNoteButtonLabel]-&gt;=0-|</string>
			<string>H:|-&gt;=0-[locationSavedMessage]-&gt;=0-[imageSavedMessage]-&gt;=0-|</string>
			<string>H:|-&gt;=0-[voiceMemoSavedMessage]-&gt;=0-[noteSavedMessage]-&gt;=0-|</string>
			<string>V:|-100-[gpsLocationButton(120)][gpsLocationButtonLabel][locationSavedMessage(21)]-10-[recordVoiceMemoButton(120)][recordVoiceMemoButtonLabel][voiceMemoSavedMessage(21)]-40-|</string>
			<string>V:|-100-[takePictureButton(120)][takePictureButtonLabel][imageSavedMessage(21)]-10-[writeNoteButton(120)][writeNoteButtonLabel][noteSavedMessage(21)]-40-|</string>
			<string>V:|-93-[tile]-35-|</string>
			<string>H:|[tile]|</string>
			<string>#gpsLocationButton.width=containerView.width,0.5,0</string>
			<string>#takePictureButton.width=containerView.width,0.5,0</string>
			<string>#recordVoiceMemoButton.width=containerView.width,0.5,0</string>
			<string>#writeNoteButton.width=containerView.width,0.5,0</string>
			<string>#parkingIcon.top=parkingReminderTitle.top,1,0</string>
			<string>#gpsLocationButton.centery=takePictureButton.centery,1,0</string>
			<string>#recordVoiceMemoButton.centery=writeNoteButton.centery,1,0</string>
			<string>#gpsLocationButtonLabel.centerx=gpsLocationButton.centerx,1,0</string>
			<string>#takePictureButtonLabel.centerx=takePictureButton.centerx,1,0</string>
			<string>#recordVoiceMemoButtonLabel.centerx=recordVoiceMemoButton.centerx,1,0</string>
			<string>#writeNoteButtonLabel.centerx=writeNoteButton.centerx,1,0</string>
			<string>#locationSavedMessage.centerx=gpsLocationButtonLabel.centerx,1,0</string>
			<string>#imageSavedMessage.centerx=takePictureButton.centerx,1,0</string>
			<string>#voiceMemoSavedMessage.centerx=recordVoiceMemoButton.centerx,1,0</string>
			<string>#noteSavedMessage.centerx=writeNoteButton.centerx,1,0</string>
		</array>
	</dict>
	<key>background</key>
	<dict>
		<key>image</key>
		<string>Default.Background.png</string>
	</dict>
	<key>parkingReminderTitle</key>
	<dict>
		<key>fontName</key>
