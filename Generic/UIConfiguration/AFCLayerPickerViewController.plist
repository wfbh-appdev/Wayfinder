<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>view</key>
	<dict>
		<key>constraints</key>
		<array>
			<string>H:|-(&gt;=50)-[noResultsFoundLabel]-(&gt;=50)-|</string>
			<string>V:|-(&lt;=100)-[noResultsFoundLabel(21)]-(&gt;=100)-|</string>
			<string>#noResultsFoundLabel.centerx=self.centerx,1,0</string>
		</array>
	</dict>
	<key>table</key>
	<dict/>
	<key>titleLabel</key>
	<dict>
		<key>type</key>
		<string>DefaultTitleLabel</string>
	</dict>
	<key>noResultsFoundLabel</key>
	<dict>
		<key>text</key>
		<string>#afcdirectoryviewcontroller.noresultsfound.title</string>
		<key>fontName</key>
		<string>$MainFont</string>
		<key>fontSize</key>
		<integer>18</integer>
		<key>color</key>
		<string>$LightGrayColor</string>
		<key>alignment</key>
		<string>center</string>
	</dict>
</dict>
</plist>
